{
  "HTML": {
    "beautifiers": [
      "Pretty Diff",
      "JS-Beautify"
    ],
    "indent_size": 4,
    "max_preserve_newlines": 1,
    "quotes": "double",
    "wrap_line_length": 80,
    "indent_inner_html": true,
    "newline_before_tags": [
      "body",
      "head",
      "main",
      "header",
      "aside",
      "nav",
      "footer"
    ]
  },
  "CSS": {
    "beautifiers": [
      "Prettier",
      "Pretty Diff",
      "JS-Beautify"
    ],
    "indent_size": 4,
    "max_preserve_newlines": 1,
    "quotes": "double",
    "wrap_line_length": 80,
    "end_with_semicolon": true
  },
  "JavaScript": {
    "beautifiers": [
      "Prettier",
      "Pretty Diff",
      "JS-Beautify",
      "ESLint"
    ],
    "end_with_comma": true,
    "indent_size": 4,
    "max_preserve_newlines": 1,
    "quotes": "double",
    "wrap_line_length": 80,
    "end_with_semicolon": true,
    "space_before_conditional": true,
    "space_in_paren": false,
    "space_in_empty_paren": false
  }
}
